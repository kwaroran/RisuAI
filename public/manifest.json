{
    "name": "Risuai",
    "icons": [
        {
            "src": "logo_512.png",
            "type": "image/png",
            "sizes": "512x512"
        },
        {
            "src": "logo_192.png",
            "type": "image/png",
            "sizes": "192x192"
        },
        {
            "src": "logo_16.png",
            "type": "image/png",
            "sizes": "16x16"
        },
        {
            "src": "logo_32.png",
            "type": "image/png",
            "sizes": "32x32"
        },
        {
            "src": "logo_256.png",
            "type": "image/png",
            "sizes": "256x256"
        }
    ],
    "start_url": "/",
    "display": "standalone",
    "theme_color": "#4682B4",
    "share_target": {
        "action": "/receive-files/",
        "method": "POST",
        "enctype": "multipart/form-data",
        "params": {
            "files": [
                {
                    "name": "character",
                    "accept": [".charx"]
                },
                {
                    "name": "preset",
                    "accept": [".risup"]
                },
                {
                    "name": "module",
                    "accept": [".risum"]
                }
            ]
        }
    },
    "file_handlers": [
        {
            "action": "/",
            "accept": {
                "application/octet-stream": [".charx", ".risup", ".risum"]
            }
        }
    ]
      
}